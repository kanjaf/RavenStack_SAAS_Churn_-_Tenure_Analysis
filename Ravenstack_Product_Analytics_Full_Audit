    {
      "cell_type": "code",
      "source": [
        "# 1. Create a flag at the subscription level: Did they ever use a beta feature?\n",
        "beta_users = feature_analysis.groupby('subscription_id')['is_beta_feature'].any().reset_index()\n",
        "beta_users.columns = ['subscription_id', 'ever_used_beta']\n",
        "\n",
        "# 2. Merge this back with your main churn data\n",
        "beta_impact = feature_analysis.merge(beta_users, on='subscription_id', how='left').fillna(False)\n",
        "\n",
        "# 3. Calculate Churn Rate for Beta vs. Non-Beta users\n",
        "beta_churn_report = beta_impact.groupby('ever_used_beta')['churn_flag'].value_counts(normalize=True).unstack() * 100\n",
        "print(beta_churn_report)"
      ],
