        "# 1. First, link Subscriptions to Accounts to get the churn_flag\n",
        "# We need subscriptions_df because it's the bridge between usage and accounts\n",
        "sub_bridge = dataframes['subscriptions_df'][['subscription_id', 'account_id']]\n",
        "account_churn = df_final[['account_id', 'churn_flag']] # df_final has your cleaned churn labels\n",
        "\n",
        "# Create the bridge: subscription_id + churn_flag\n",
        "sub_churn_map = sub_bridge.merge(account_churn, on='account_id')\n",
        "\n",
        "# 2. Now merge feature usage with our map\n",
